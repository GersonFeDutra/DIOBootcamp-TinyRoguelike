; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Tiny Roguelike"
config/description="A tiny swords adventure, inspired on the tiny swords asset pack from Pixel Frog, make as a project from the bootcamp of game development with Godot from the platforme DIO.me."
run/main_scene="res://world.tscn"
config/features=PackedStringArray("4.2")
config/icon="res://icon.png"
run/ignore_damage=false
run/time_scale=1.0
run/first_play=true
run/enable_screenshot=true

[autoload]

ControllsGuide="*res://src/autoload/controlls_guide.tscn"
DevMode="*res://src/autoload/DevMode.tscn"
LocalizationManager="*res://addons/localization_editor/LocalizationManager.gd"

[display]

window/stretch/mode="canvas_items"
mouse_cursor/custom_image="res://addons/Tiny Swords/UI/Pointers/cursor.tres"
mouse_cursor/custom_image_hotspot=Vector2(2, 2)

[editor_plugins]

enabled=PackedStringArray("res://addons/localization_editor/plugin.cfg")

[file_customization]

folder_colors={
"res://src/areas/": "blue",
"res://src/autoload/": "gray",
"res://src/characters/": "yellow",
"res://src/fx/": "purple",
"res://src/hud/": "teal",
"res://src/lib/": "gray"
}

